{
  "routes": [
    {
      "src": "/api/videos",
      "headers": {
        "Connection": "keep-alive",
        "Keep-Alive": "timeout=60",
        "x-vercel-max-body-size": "100mb"
      }
    }
  ],
  "functions": {
    "app/api/videos/route.ts": {
      "memory": 1024,
      "maxDuration": 60
    }
  },
  "build": {
    "env": {
      "NEXT_PRIVATE_STANDALONE": "true"
    }
  }
}
